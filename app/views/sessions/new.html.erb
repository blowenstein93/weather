<% provide(:title, "Sign in") %>
<h1>Sign in</h1>


<%= form_for(:session, url: sessions_path) do |s| %>
    <%= s.label :email %>
    <%= s.text_field :email %>

    <%= s.label :phone %>
    <%= s.number_field :phone %>

    <%= s.submit "Sign-In" %>
<% end %>


<p>New user? <%= link_to "Sign up now!", signup_path %></p>
